<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jorgebarcena.IAApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>JorgeIA</name>
    <description>
       Algunos algoritmos de Inteligencia artificial aplicados a juegos simples
    </description>
    <author email="j.barcenalumbreras@gmail.com" href="http://www.jorgebarcenadev.com">
        Jorge BÃ¡rcena Lumbreras
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="19" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <icon src="res/icon.png" />
    <engine name="android" spec="^8.1.0" />
</widget>
